<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             version="2.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd
   http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
    <portlet>
        <portlet-name>WhoIsOnLinPortlet</portlet-name>
        <display-name xml:lang="EN">Who is on Line</display-name>
        <portlet-class>org.exoplatform.platform.portlet.juzu.whoisonline.WhoIsOnlinePortlet</portlet-class>
        <init-param>
            <name>juzu.run_mode</name>
            <value>prod</value>
        </init-param>
        <init-param>
            <name>juzu.inject</name>
            <value>spring</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>Who is on line</title>
            <keywords>juzu</keywords>
        </portlet-info>
    </portlet>
    <portlet>
        <portlet-name>GettingStartedPortlet</portlet-name>
        <display-name xml:lang="EN">Getting Started</display-name>
        <portlet-class>org.exoplatform.platform.portlet.juzu.gettingstarted.GettingstartedPortlet</portlet-class>
        <init-param>
            <name>juzu.run_mode</name>
            <value>prod</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>getting Started</title>
            <keywords>Home_Page_Portlets</keywords>
        </portlet-info>
    </portlet>
    <portlet>
        <portlet-name>SuggestionsPeopleSpace</portlet-name>
        <display-name xml:lang="EN">Suggestions Portlet</display-name>
        <portlet-class>org.exoplatform.platform.portlet.juzu.suggestions.SuggestionsPortlet</portlet-class>
        <init-param>
            <name>juzu.run_mode</name>
            <value>prod</value>
        </init-param>
        <init-param>
            <name>juzu.inject</name>
            <value>spring</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>Suggestions</title>
            <keywords>juzu</keywords>
        </portlet-info>
    </portlet>
    <portlet>
        <portlet-name>Invitations</portlet-name>
        <display-name xml:lang="EN">Invitations</display-name>
        <portlet-class>org.exoplatform.platform.portlet.juzu.invitations.InvitationsPortlet</portlet-class>
        <init-param>
            <name>juzu.run_mode</name>
            <value>dev</value>
        </init-param>
        <init-param>
            <name>juzu.inject</name>
            <value>spring</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>Invitations</title>
            <keywords>juzu</keywords>
        </portlet-info>
    </portlet>
    <portlet>
        <portlet-name>HomePageCalendarPortlet</portlet-name>
        <display-name xml:lang="EN">Calendar Portlet</display-name>
        <portlet-class>org.exoplatform.platform.portlet.juzu.calendar.CalendarPortlet</portlet-class>
        <init-param>
            <name>juzu.run_mode</name>
            <value>prod</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>Calendar Portlet</title>
            <keywords>Home_Page_Portlets_Calendar</keywords>
        </portlet-info>
    </portlet>
</portlet-app>