<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Pembahasan</topic>
         <poll>Pemilihan</poll>
         <event>Kegiatan</event>
         <upload>Berkas</upload>
         <wiki>Wiki Halaman</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>Pembahasan</topic>
         <poll>Pemilihan</poll>
         <CreateANew>Terbaru {1}:</CreateANew>
         <NoForum>Maaf, tidak ada forum yang tersedia namun Anda bisa membuat {1}. Mulailah dengan membuat ruang pertama terlebih dahulu lalu coba lagi.</NoForum>
         <RequireSelectForum>Silahkan memilih sebuah forum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Memilih Forum</SelectAForum>
         <location>Di lokasi</location>
         <forumId>Dan Forum</forumId>
         <FilterForums>Menyaring forum</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Pilih forum</SelectAForum>
         <location>Di lokasi</location>
         <forumId>Dan Forum</forumId>
         <FilterForums>Menyaring forum</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>Halaman Baru Wiki:</title>
         <location>Lokasi</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Buat kegiatan baru</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Tambahan baru:</Choix>
         <EndEvent>Untuk</EndEvent>
         <StartEvent>Dari</StartEvent>
         <Calendar>Kalendar</Calendar>
         <all-day>Sepanjang hari</all-day>
         <defaultCalendarId>Standar</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Bidang "Judul" diperlukan.</summary-field-required>
         <fromDate-format>Format tanggal "Dari" salah.</fromDate-format>
         <toDate-format>Format tanggal "kepada" tidak benar.</toDate-format>
         <logic-required>Maaf, Anda tidak dapat mengakhiri acara yang belum dimulai.</logic-required>
         <cant-add-event-on-remote-calendar>Anda tidak dapat menambahkan acara yang masih jauh dari tanggalnya</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>Acara ditambahkan kedalam {1}</Event>
            <Task>Tugas ditambahkan kedalam {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} tidak ada penambahan.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Pilih file</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Kegiatan</Event>
         <Task>Tugas</Task>
      </label>
   </Choix>
</bundle>
