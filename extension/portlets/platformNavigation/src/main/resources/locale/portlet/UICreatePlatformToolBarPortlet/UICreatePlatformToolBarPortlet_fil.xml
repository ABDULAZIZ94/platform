<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Talakayan</topic>
         <poll>Botohan</poll>
         <event>Kaganapan</event>
         <upload>File</upload>
         <wiki>Pahina ng wiki</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>talakayan</topic>
         <poll>Botohan</poll>
         <CreateANew>Bago {1}:</CreateANew>
         <NoForum>Paumanhin, walang pang forum na magagamit sa ngayon kung saan maaari kang lumikha ng mga {1}. Magsimula sa pamamagitan ng paglikha ng espasyo sa una at pagkatapos ay subukang muli.</NoForum>
         <RequireSelectForum>Mangyaring pumili ng isang forum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Piliin ang forum</SelectAForum>
         <location>Sa lokasyon</location>
         <forumId>At Forum</forumId>
         <FilterForums>I-filter ayon sa mga forum</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Piliin ang forum</SelectAForum>
         <location>Sa lokasyon</location>
         <forumId>At Forum</forumId>
         <FilterForums>I-filter ayon sa mga forum</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>Bagong pahina sa Wiki:</title>
         <location>Lokasyon</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Gumawa ng bagong kaganapan</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Magdagdag ng bago:</Choix>
         <EndEvent>Kay</EndEvent>
         <StartEvent>Galing sa</StartEvent>
         <Calendar>Kalendaryo</Calendar>
         <all-day>Buong araw</all-day>
         <defaultCalendarId>Default</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Ang patlang na "Title" ay kinakailangan.</summary-field-required>
         <fromDate-format>Ang format ng petsa ng "From" ay hindi tama.</fromDate-format>
         <toDate-format>Ang format ng petsa ng "To" ay hindi tama.</toDate-format>
         <logic-required>Paumanhin, hindi ka makakagawa ng pangyayari na nagtapos na bago nagsimula.</logic-required>
         <cant-add-event-on-remote-calendar>You cannot add event to remote calendar</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>The Event was added to {1}</Event>
            <Task>The Task was added to {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} was not added.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Select File</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Event</Event>
         <Task>Task</Task>
      </label>
   </Choix>
</bundle>
