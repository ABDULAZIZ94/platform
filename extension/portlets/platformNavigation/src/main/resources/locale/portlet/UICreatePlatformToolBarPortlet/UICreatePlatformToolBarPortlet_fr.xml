         </cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>L’événement est ajouté dans {1}.</Event>
            <Task>La tâche est ajoutée dans {1}.</Task>
         </add-successfully>
         <add-unsuccessfully>L'ajout{0} est echoué.</add-unsuccessfully>
      </msg>
      <action>
         <Next>Suivant</Next>
         <Cancel>Annuler</Cancel>
      </action>
