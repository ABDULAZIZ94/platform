<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Criar Sondagem</poll>
         <event>Criar Evento</event>
         <upload>File</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>discussion</topic>
         <poll>Poll</poll>
         <CreateANew>Criar {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet in which you can create the {1}. Start by creating a space first and then try again.</NoForum>
         <RequireSelectForum>Please select a forum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Selecione um fórum</SelectAForum>
         <location>Local</location>
         <forumId>Fórum</forumId>
         <FilterForums>Filtrar fóruns</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Select Forum</SelectAForum>
         <location>Local</location>
         <forumId>Fórum</forumId>
         <FilterForums>Filtrar fóruns</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>New Wiki Page:</title>
         <location>Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Title>#{word.title}</Title>
         <Choix>Add New:</Choix>
         <EndEvent>a</EndEvent>
         <StartEvent>From</StartEvent>
         <Calendar>Calendar</Calendar>
         <all-day>O dia todo</all-day>
         <defaultCalendarId>Padrão</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>O campo "Título" é obrigatório.</summary-field-required>
         <fromDate-format>Formato da data de início incorreto.</fromDate-format>
         <toDate-format>Formato da data de término incorreto.</toDate-format>
         <logic-required>A data de término deve ser maior do que a data de início.</logic-required>
         <cant-add-event-on-remote-calendar>Você não pode adicionar um evento ao calendário remoto</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>O evento foi adicionado a {1}</Event>
            <Task>The Task was added to {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} was not added.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Selecionar arquivo</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Evento</Event>
         <Task>Task</Task>
      </label>
   </Choix>
</bundle>
